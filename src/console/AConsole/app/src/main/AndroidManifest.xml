<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="club.rascal.notorious.aconsole">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VarioPreference"
            android:label="@string/title_activity_vario_preference" />
        <activity
            android:name=".ToneEditorActivity"
            android:label="@string/title_activity_tone_editor"/>
        <activity
            android:name=".CalibrationActivity"
            android:label="@string/title_activity_calibration"/>
    </application>

</manifest>